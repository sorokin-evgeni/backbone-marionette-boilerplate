{
    "enviroment": "prod",
    "templateEngine": "underscore",
    "templateExt": ".us",
    "staticSrc": {
        "base": "public",
        "css": "public/css",
        "js": "public/js",
        "templates": "public/templates",
        "includeModules": ["js/app.js", "js/app/modules/user"]
    },
    "staticDist": {
        "tmpFolder": "dist/tmp",
        "base": "dist/public",
        "css": "dist/public/css",
        "js": "dist/public/js",
        "cssBundleName": "main.css",
        "jsBundleName": "app.js"
    }

}